{
    "name": "eturiatravel/aws-cognito",
    "description": "AWS Cognito package that allows Auth and other related features using the AWS SDK for PHP",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/eturiatravel/aws-cognito",
    "authors": [
        {
            "name": "EllaiSys",
            "role": "owner"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "stable",
    "require": {
        "php": "^7.0|^8.0",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "aws/aws-sdk-php": ">=3.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.0"
    },
    "autoload": {
        "psr-4": {
          "Ellaisys\\Cognito\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ellaisys\\Cognito\\Providers\\AwsCognitoServiceProvider"
            ],
            "aliases": {
            }
        },
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
